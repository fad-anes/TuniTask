<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="gui.Addanswer"
            prefHeight="400.0" prefWidth="600.0">
    <children>


        <Label fx:id="idanswer" layoutX="16.0" layoutY="58.0" text="Answer" />
        <Label fx:id="idiscorrect" layoutX="14.0" layoutY="186.0" text="Is it correct" />

        <TextField fx:id="answertextfield" layoutX="163.0" layoutY="55.0" prefHeight="33.0" prefWidth="383.0" />
         <TextField fx:id="iscorrecttextfield" layoutX="163.0" layoutY="183.0" prefHeight="33.0" prefWidth="383.0" />

        <Button fx:id="idbtn" layoutX="54.0" layoutY="267.0" mnemonicParsing="false" onAction="#insertanswer" text="Add answer" />
        <Button fx:id="idcancel" layoutX="248.0" layoutY="267.0" mnemonicParsing="false" onAction="#readAll" text="Back to questions" />

    </children>
</AnchorPane>
