<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="gui.Editanswer"
            prefHeight="400.0" prefWidth="600.0">

    <Label layoutX="26.0" layoutY="44.0" text="Answer:" />
    <TextField fx:id="answerTextField" layoutX="110.0" layoutY="41.0" prefHeight="25.0" prefWidth="380.0" />

    <Label layoutX="26.0" layoutY="92.0" text="Is Correct:" />
    <TextField fx:id="isCorrectTextField" layoutX="110.0" layoutY="89.0" prefHeight="25.0" prefWidth="380.0" />

    <Button fx:id="saveButton" layoutX="226.0" layoutY="147.0" mnemonicParsing="false" onAction="#save" text="Save" />
    <Button fx:id="cancelButton" layoutX="325.0" layoutY="147.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" />

</AnchorPane>
