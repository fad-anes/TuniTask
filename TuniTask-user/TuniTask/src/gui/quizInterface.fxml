<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.AddQuiz">
   <stylesheets>
      <URL value="@styles.css" />
   </stylesheets>
   <children>
      <Label fx:id="idquiztitle" layoutX="16.0" layoutY="58.0" text="Quiz Title" />
      <Label fx:id="iddescription" layoutX="11.0" layoutY="116.0" text="Quiz Description" />
      <Label fx:id="idscore" layoutX="14.0" layoutY="186.0" text="Score" />
      <TextField fx:id="titletextfield" layoutX="163.0" layoutY="55.0" prefHeight="33.0" prefWidth="383.0" />
      <TextField fx:id="desctextfield" layoutX="163.0" layoutY="113.0" prefHeight="34.0" prefWidth="383.0" />
      <TextField fx:id="scoretextfield" layoutX="163.0" layoutY="183.0" prefHeight="33.0" prefWidth="383.0" />
      <Button fx:id="idbtn" layoutX="54.0" layoutY="267.0" mnemonicParsing="false" onAction="#insert" text="Insert Quiz" />
      <Button fx:id="idbtndisplayall" layoutX="248.0" layoutY="267.0" mnemonicParsing="false" onAction="#readAll" text="Display Quizs" />

   </children>
</AnchorPane>
